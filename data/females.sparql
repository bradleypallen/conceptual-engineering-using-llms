SELECT ?item ?name ?article ?gender WHERE {
  ?item wdt:P21 wd:Q6581072 .
  ?article schema:about ?item ;
           schema:inLanguage "en" ;
           schema:isPartOf <https://en.wikipedia.org/> ;
           schema:name ?name .
  wd:Q6581072 rdfs:label ?gender .
  FILTER (lang(?gender) = "en")
}
LIMIT 1000